{
  "name": "logofie-marketplace-2026-premium",
  "version": "2026.01.1",
  "description": "Plateforme e-commerce Logofiè - Édition Premium 2026 avec IA avancée",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec node --loader ts-node/esm src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc && tsc-alias",
    "build:prod": "npm run build && npm prune --production",
    "test": "NODE_ENV=test jest --coverage",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t logofie-marketplace:2026-premium .",
    "docker:run": "docker run -p 5002:5002 --env-file .env.production logofie-marketplace:2026-premium",
    "docker:compose": "docker-compose up -d",
    "migrate:up": "node --loader ts-node/esm src/database/migrations/up.ts",
    "migrate:down": "node --loader ts-node/esm src/database/migrations/down.ts",
    "seed": "node --loader ts-node/esm src/database/seeds/seed.ts"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.49.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.49.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.49.0",
    "@opentelemetry/resources": "^1.24.0",
    "@opentelemetry/sdk-metrics": "^1.24.0",
    "@opentelemetry/sdk-node": "^0.49.0",
    "@opentelemetry/sdk-trace-node": "^1.24.0",
    "@opentelemetry/semantic-conventions": "^1.24.0",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.4.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.11",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "e-commerce",
    "marketplace",
    "ai",
    "recommendations",
    "nodejs",
    "typescript",
    "redis",
    "opentelemetry"
  ],
  "author": "Logofiè Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/logofie/marketplace-2026-premium"
  }
}
